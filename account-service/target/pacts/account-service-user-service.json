{
    "provider": {
        "name": "user-service"
    },
    "consumer": {
        "name": "account-service"
    },
    "interactions": [
        {
            "description": "a request to get an user",
            "request": {
                "method": "GET",
                "path": "/users/\ubd1c\u8fe7",
                "matchingRules": {
                    "path": {
                        "matchers": [
                            {
                                "match": "regex",
                                "regex": "/users/.*"
                            }
                        ],
                        "combine": "AND"
                    }
                }
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "name": "gopinath",
                    "id": "6eea92a2-929b-467b-9fca-15f057b7edd7",
                    "email": "gopinath@gmail.com"
                },
                "matchingRules": {
                    "body": {
                        "$.id": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.name": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.email": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            }
        }
    ],
    "metadata": {
        "pactSpecification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.6.1"
        }
    }
}